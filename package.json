  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "~4.16.0",
    "mongoose": "^5.11.15",
    "morgan": "~1.9.0",
    "nodemon": "^2.0.7"
  }
}
